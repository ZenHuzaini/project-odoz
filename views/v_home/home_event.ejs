<section class="text-center py-5 indigo darken-1 text-white" id="pricing">
    <div class="container">
      <div class="wow fadeIn">
        <h2 class="h1 pt-5 pb-3">Our Events</h2>
        <p class="px-5 mb-5 pb-3 lead">
          We habe thousands of event where you can participate in. Check the events below,
          Maybe you are interested :) .. 
        </p>
      </div>
      <div class="row wow zoomIn">
        <% for (var i = 0; i < seeAllEvent.length; i++){ %>
        <div class="col-lg-4 col-md-12 mb-r">
          <div class="card card-image">
            <div class="text-white text-center pricing-card d-flex align-items-center rgba-stylish-strong py-3 px-3 rounded">
              <div class="card-body">
                <div class="h4"><%= seeAllEvent[i].event_title %></div>
                <div class="h5"><span class="display-5"><%= seeAllEvent[i].event_date %></span></div>
                <div class="h6"><span class="display-4"><%= seeAllEvent[i].event_time %></span></div>
                <!-- <div class="py-5"><span class="display-4"><%= seeAllEvent[i].event_location %></span></div> -->
                <ul class="list-unstyled">
                  <li>
                    <a class="btn btn-outline-white mt-5" href="event/vieweventodoz/<%= seeAllEvent[i]._id %>"> Interested</a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
        <% } %>
          <a class="btn btn-outline-white mt-5 align-items-center text-center" href="event/listeventodoz"> More Events</a>
      </div>
    </div>
</section>

